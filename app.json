{
  "expo": {
    "scheme": "solimbo",
    "name": "Solimbo",
    "slug": "Solimbo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main_logo_no_bg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main_logo_no_bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#191414"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solimbo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main_logo_v1_500x500.png",
        "backgroundColor": "#206f3e"
      },
      "package": "com.solimbo.app"
    },
    "extra": {
      "eas": {
        "projectId": "99fd1334-56da-4b89-a868-0da4ee287c7d"
      }
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ]
  }
}
